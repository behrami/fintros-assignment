<div class= 'container'>
  JOB STORIES

  <ul>
  <% @job_stories_array.each do |story_hash| %>
    <li>
      <%= link_to(story_hash[:title], story_hash[:url]) %><br>
      <%= story_hash[:score]%> points by <%= story_hash[:author]%> | <%= story_hash[:comments_num]%> comments
    </li>
  <% end %>
</ul>
</div>
